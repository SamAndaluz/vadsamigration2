<odoo>
  <data>

    <record id="view_stock_move_operations_custom" model="ir.ui.view">
      <field name="name">stock.move.operations.form.custom</field>
      <field name="model">stock.move</field>
      <field name="inherit_id" ref="stock.view_stock_move_operations"/>
      <field name="arch" type="xml">
        <xpath expr="//form/group[1]" position="after">
          <field name="picking_type_code" invisible="1"/>
          <field name="state" invisible="1"/>
          <group attrs="{'invisible': ['|',('picking_type_code', '!=', 'incoming'),('state', 'not in', ('draft'))]}">
            <group>
              <label for="serial_numbers_file"/>
              <div class="o_row">
                <span><field name="serial_numbers_file_name" invisible="1"/></span>
                <span><field name="serial_numbers_file" filename="serial_numbers_file_name" nolabel="1"/></span>
                <span><button name="load_serial_numbers" type="object" class="oe_link" string="Procesar" attrs="{'invisible': [('serial_numbers_file','=',False)]}"/></span>
                <span><button name="reset_serial_number" type="object" class="oe_link" string="Resetear NÃºmeros de Serie"/></span>
              </div>
              <!--  
              <field name="serial_numbers_file_name" invisible="1"/>
              <field name="serial_numbers_file" filename="serial_numbers_file_name"/>
              -->
            </group>
          </group>
        </xpath>
      </field>
    </record>

  </data>
</odoo>